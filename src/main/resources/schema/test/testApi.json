    "parameters" : [
      {"paramName" : "page", "required" : true}
    ],
    "config" : {
      "createItem" : {
        "typeId": "testItem",
        "event": "create",
        "if" : "",
        "data": {
          "id": "{{:id}}",
          "category": "{{:category}}",
          "title": "{{:title}}"
        }
      },
      "categories" : {
        "typeId":"testCategory",
        "select" : {"sql" : "select * from testCategory where categoryType = @{categoryType}", "ds": "mariaDb"},
        "response" : {
          "categoryName": "",
          "upperCategory": "parentCategory",
          "refCode1": {
            "typeId": "testItem",
            "query": {"category": "{{:default(categoryId, drama)}}", "sorting" : "parentCategory"},
            "response" : {
              "category": "",
              "itemTitle": "title"
            }
          }
        }
      },
      "items" : {
        "typeId":"testItem",
        "query" : [
          {"field" : "category", "method" : "matching", "value" : "drama" },
          {"sorting" : "id desc"},
          {"page" : "{{:itemsPage}}", "pageSize":20}
        ]
      },
      "users" : {
        "endpoint":"http://customer.api.com/api/test.json?id={{:testId}}"
      }
    }
  },
