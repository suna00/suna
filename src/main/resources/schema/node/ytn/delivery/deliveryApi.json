        }
      },
      "tobeSetY" : {
        "typeId": "myDeliveryAddress",
        "event": "update",
        "data": {
          "myDeliveryAddressId": "{{:tobeId}}",
          "defaultYn": "y",
          "changed": ""
        }
      }
    }
  }
