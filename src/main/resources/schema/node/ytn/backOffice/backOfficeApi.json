          "sql" : "select FLOOR(1000 + (RAND()*8999)) as certCode "
        }
      },
      "createHistory" : {
        "typeId": "smsCertification",
        "event": "create",
        "data": {
          "smsCertificationId": "{{:smsCertificationId}}",
          "name": "test",
          "smsCertificationType": "{{:type}}",
          "adminId": "{{:adminId}}",
          "memberNo": "{{:memberNo}}",
          "cellphone": "{{:phone}}",
          "certCode": "{{:certCode}}",
          "certRequestDate": "{{:now}}"
        }
      }
    }
  }
