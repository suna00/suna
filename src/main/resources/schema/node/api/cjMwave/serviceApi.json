[
  {
    "typeId": "apiConfig",
    "category": "comp",
    "apiId": "IfCom001",
    "apiName": "서비스 컴포넌트 상세조회",
    "method": "GET",
    "parameters": [
      {
        "paramName": "page",
        "required": false
      },
      {
        "paramName": "pageSize",
        "required": false
      },
      {
        "paramName": "sorting",
        "required": false
      },
      {
        "paramName": "langCd",
        "required": false
      },
      {
        "paramName": "mainDivCd",
        "required": true
      },
      {
        "paramName": "showLoCd",
        "required": true
      }
    ],
    "config": {
      "typeId": "compItemInfo",
      "query": [
        {
          "field": "mainSeq",
          "method": "referenceJoin",
          "value": "mainDivCd_matching={{:mainDivCd}}&showLoCd_matching={{:showLoCd}}&showYn_matching=true&limit=1&sorting=mainSeq"
        },
        {
          "referenceView": "itemId,cnctContsId",
          "includeReferenced": true
        }
      ],
      "response": {
        "compItemSeq": "",
        "itemDivCd": "",
        "itemNm": "",
        "subName": "",
        "itemId": "",
        "typeCd": "",
        "ifTgtCd": "",
        "sortOdrg": "",
        "showCntryCdList": "",
        "bgOptnYn": "",
        "refdAtribList": {
          "typeId": "compAtribInfo",
          "query": [
            {
              "field": "compItemSeq",
              "method": "referenceJoin",
              "value": "compItemSeq_matching={{:compItemSeq}}&cnctItemYn_matching=false"
            },
            {
              "includeReferenced": true
            }
          ],
          "response": {
            "compAtribSeq": "",
            "compItemSeq": "",
            "cnctItemYn": "",
            "contsId": "",
            "atribNm": "",
            "imgPath": "",
            "videFilePath": "",
            "newsHubKywrd": "",
            "newsHubNewsList": {
              "typeId": "news",
              "query": [
                {
                  "field": "newsNm",
                  "method": "matchingShould",
                  "value": "{{:default(newsHubKywrd,뺵뺵)}}"
                },
                {
                  "field": "findKywrd",
                  "method": "matchingShould",
                  "value": "{{:default(newsHubKywrd,뺵뺵)}}"
                }
              ]
            },
            "newsHubVodList": {
              "typeId": "pgmVideo",
              "query": [
                {
                  "field": "title",
                  "method": "matchingShould",
                  "value": "{{:default(newsHubKywrd,뺵뺵)}}"
                },
                {
                  "field": "searchKeyword",
                  "method": "matchingShould",
                  "value": "{{:default(newsHubKywrd,뺵뺵)}}"
                }
              ]
            }
          }
        },
        "cnctContsDivCd": "",
        "cnctContsNm": "",
        "cnctContsIfTgtCd": "",
        "cnctContsTypeCd": "",
        "cnctContsLeftYn": "",
        "cnctContsShowCntryCdList": "",
        "cnctContsId": "",
        "cnctBgOptnYn": "",
        "cnctRefdAtribList": {
          "typeId": "compAtribInfo",
          "query": [
            {
              "field": "compItemSeq",
              "method": "referenceJoin",
              "value": "compItemSeq_matching={{:compItemSeq}}&cnctItemYn_matching=true"
            },
            {
              "includeReferenced": true
            }
          ],
          "response": {
            "compAtribSeq": "",
            "compItemSeq": "",
            "cnctItemYn": "",
            "contsId": "",
            "atribNm": "",
            "imgPath": "",
            "videFilePath": "",
            "newsHubKywrd": "",
            "newsHubNewsList": {
              "typeId": "news",
              "query": [
                {
                  "field": "newsNm",
                  "method": "matchingShould",
                  "value": "{{:default(newsHubKywrd,뺵뺵)}}"
                },
                {
                  "field": "findKywrd",
                  "method": "matchingShould",
                  "value": "{{:default(newsHubKywrd,뺵뺵)}}"
                }
              ]
            },
            "newsHubVodList": {
              "typeId": "pgmVideo",
              "query": [
                {
                  "field": "title",
                  "method": "matchingShould",
                  "value": "{{:default(newsHubKywrd,뺵뺵)}}"
                },
                {
                  "field": "searchKeyword",
                  "method": "matchingShould",
                  "value": "{{:default(newsHubKywrd,뺵뺵)}}"
                }
              ]
            }
          }
        }
      }
    }
  },
  {
    "typeId": "apiConfig",
    "category": "comp",
    "apiId": "IfCom002",
    "apiName": "서비스 메뉴 상세조회",
    "method": "GET",
    "parameters": [
      {
        "paramName": "page",
        "required": false
      },
      {
        "paramName": "pageSize",
        "required": false
      },
      {
        "paramName": "sorting",
        "required": false
      },
      {
        "paramName": "langCd",
        "required": false
      },
      {
        "paramName": "cntryCd",
        "required": true
      },
      {
        "paramName": "svcTermlDivCd",
        "required": true
      }
    ],
    "config": {
      "root": {
        "typeId": "svcMenuInfo",
        "query": [
          {
            "field": "svcMenuGroupSeq",
            "method": "referenceJoin",
            "value": "showCntryCdList_matching={{:cntryCd}}&svcDvcDivCd_matching={{:svcTermlDivCd}}&showYn_matching=true&limit=1"
          },
          {
            "includeReferenced": true
          }
        ]
      },
      "anchor": {
        "typeId": "banrBasInfo",
        "query": [
          {
            "field": "svcTermlDivCd",
            "method": "matching",
            "value": "{{:svcTermlDivCd}}"
          },
          {
            "field": "showCntryCdList",
            "method": "matching",
            "value": "{{:cntryCd}}"
          },
          {
            "field": "banrTypeCd",
            "method": "matching",
            "value": "4"
          },
          {
            "field": "showYn",
            "method": "matching",
            "value": "true"
          },
          {
            "sorting": "banrSeq desc"
          },
          {
            "includeReferenced": true
          }
        ]
      }
    }
  }
]
