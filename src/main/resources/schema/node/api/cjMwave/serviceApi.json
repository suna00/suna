[
  {
    "typeId": "apiConfig",
    "category": "comp",
    "apiId": "IfCom001",
    "apiName": "서비스 컴포넌트 상세조회",
    "method": "GET",
    "parameters": [
      {
        "paramName": "page",
        "required": false
      },
      {
        "paramName": "pageSize",
        "required": false
      },
      {
        "paramName": "sorting",
        "required": false
      },
      {
        "paramName": "langCd",
        "required": false
      },
      {
        "paramName": "divCd",
        "required": false
      },
      {
        "paramName": "showLoCd",
        "required": false
      }
    ],
    "config": {
      "typeId": "mainBasInfo",
      "query": [
        {
          "field": "mainDivCd",
          "method": "matching",
          "value": "{{:divCd}}"
        },
        {
          "field": "showLoCd",
          "method": "matching",
          "value": "{{:showLoCd}}"
        },
        {
          "field": "showYn",
          "method": "matching",
          "value": "true"
        },
        {
          "sorting": "mainSeq desc"
        },
        {
          "limit": "1"
        },
        {
          "includeReferenced": true
        }
      ],
      "response": {
        "mainSeq": "",
        "mainNm": "",
        "refdCompItemList": {
          "referenceView": true,
          "includeReferenced": false
        }
      }
    }
  },
  {
    "typeId": "apiConfig",
    "category": "comp",
    "apiId": "IfCom002",
    "apiName": "서비스 메뉴 상세조회",
    "method": "GET",
    "parameters": [
      {
        "paramName": "page",
        "required": false
      },
      {
        "paramName": "pageSize",
        "required": false
      },
      {
        "paramName": "sorting",
        "required": false
      },
      {
        "paramName": "langCd",
        "required": false
      },
      {
        "paramName": "cntryCd",
        "required": true
      },
      {
        "paramName": "svcTermlDivCd",
        "required": true
      }
    ],
    "config": {
      "root": {
        "typeId": "svcMenuInfo",
        "query": [
          {
            "field": "svcMenuGroupSeq",
            "method": "referenceJoin",
            "value": "showCntryCdList_matching={{:cntryCd}}&svcDvcDivCd_matching={{:svcTermlDivCd}}&showYn_matching=true&limit=1"
          },
          {
            "includeReferenced": true
          }
        ]
      },
      "anchor": {
        "typeId": "banrBasInfo",
        "query": [
          {
            "field": "svcTermlDivCd",
            "method": "matching",
            "value": "{{:svcTermlDivCd}}"
          },
          {
            "field": "showCntryCdList",
            "method": "matching",
            "value": "{{:cntryCd}}"
          },
          {
            "field": "banrTypeCd",
            "method": "matching",
            "value": "4"
          },
          {
            "field": "showYn",
            "method": "matching",
            "value": "true"
          },
          {
            "sorting": "banrSeq desc"
          },
          {
            "includeReferenced": true
          }
        ]
      }
    }
  }
]
