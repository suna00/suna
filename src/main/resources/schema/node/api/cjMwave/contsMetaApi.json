              },
              {
                "field": "langCd",
                "method": "matching",
                "value": "{{:conditionDefault(langCd,eng,eng,zho-cn,zho-tw}}"
              },
              {
                "sorting": "mkeDay desc",
                "limit": "2"
              }
            ]
          },
          "rcmdVideoList": {
            "typeId": "videItgInfo",
            "query": [
              {
                "sorting": "created desc",
                "limit": "2"
              }
            ]
          },
          "rcmdSPhotoList": {
            "typeId": "singlePhoto",
            "query": [
              {
                "sorting": "mkeDay desc",
                "limit": "2"
              }
            ]
          },
          "rcmdVoteList": {
            "typeId": "voteBasInfo",
            "query": [
              {
                "field": "voteFormlCd",
                "method": "matching",
                "value": "voteFormlCd>99"
              },
              {
                "field": "showYn",
                "method": "matching",
                "value": "true"
              },
              {
                "sorting": "created desc",
                "limit": "3",
                "includeReferenced": true
              }
            ],
            "response": {
              "_all_": "",
              "refdItemList": {
                "typeId": "voteItemInfo",
                "query": [
                  {
                    "field": "voteSeq",
                    "method": "referenceJoin",
                    "value": "voteSeq_matching={{:voteSeq}}"
                  },
                  {
                    "sorting": "sortOdrg"
                  }
                ]
              },
              "refdSeriesItemList": {
                "typeId": "sersVoteItemInfo",
                "query": [
                  {
                    "field": "voteSeq",
                    "method": "referenceJoin",
                    "value": "voteSeq_matching={{:voteSeq}}"
                  },
                  {
                    "sorting": "sortOdrg"
                  }
                ]
              }
            }
          }
        }
      }
