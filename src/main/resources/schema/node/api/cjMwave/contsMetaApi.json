              }
            ]
          },
          "rcmdNewsList": {
            "typeId": "news",
            "query": [
              {
                "field": "relArtistIds",
                "method": "matching",
                "value": "{{:getCommaItem(prsnNo,0,뺿뺿)}}"
              },
              {
                "field": "showYn",
