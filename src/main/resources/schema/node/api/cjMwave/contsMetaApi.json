      "resultType": "none",
      "event": "view",
      "response": {
        "hitNum": ""
      }
    },
      "root": {
        "typeId": "musicVideo",
        "cacheTime": "300",
        "excludePids": "showCntryCdList",
        "id": "{{:musicVideoId}}",
        "response": {
          "_all_": "",
          "seoTitle": {
            "value": "{{:musicVideoNm}} | Mwave"
          },
          "seoDesc": "musicVideoDesc",
          "seoKywrd": "findKywrd",
          "replNum": {
            "select": {
              "ds": "authDb",
              "resultType": "value",
              "sql": "select count(*)as replNum from replInfo where contsMetaId = concat('musicVideo','::',@{musicVideoId})"
            }
          },
          "likeNum": {
            "select": {
              "ds": "authDb",
              "resultType": "value",
              "sql": "select count(*)as likeNum from contsLikeInfo where contsMetaId = concat('musicVideo','::',@{musicVideoId})"
            }
          },
          "myLikeYn": {
            "select": {
              "ds": "authDb",
              "resultType": "value",
              "sql": "select if(count(*)>0,'Y','N') myLikeYn from contsLikeInfo where contsMetaDivCd = '4' and contsMetaId = concat('musicVideo','::',@{musicVideoId}) and mbrId = @{makeIdValue(>,snsTypeCd,snsKey)}"
            }
          },
          "refMusicVideoList": {
            "typeId": "musicVideo",
            "query": [
              {
                "field": "contsMetaCtgryId",
                "method": "matchingShould",
                "value": "{{:getCommaItem(contsMetaCtgryId,last,뺿뺿)}}"
              },
              {
                "field": "showYn",
                "method": "matching",
                "value": "true"
              },
              {
                "sorting": "created desc",
                "limit": "15"
              }
            ]
          },
          "rcmdNewsList": {
            "typeId": "news",
            "query": [
              {
                "field": "apvSttusCd",
                "method": "matching",
