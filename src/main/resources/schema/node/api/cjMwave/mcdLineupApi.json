[
  {
    "typeId": "apiConfig",
    "category": "mcdlineup",
    "apiId": "IfMcl001",
    "apiName": "MCD 라인업(출연자) 상세조회",
    "method": "GET",
    "parameters": [
      {
        "paramName": "bradDate",
        "required": false
      }
    ],
    "config": {
      "typeId": "mcdLineupInfo",
      "query": [
        {
          "field": "bradDate",
          "method": "matching",
          "value": "{{:bradDate}}"
        },
        {
          "field": "showYn",
          "method": "matching",
          "value": "true"
        },
        {
          "sorting": "lineupInfoSeq desc"
        },
        {
          "limit": "1"
        },
        {
          "includeReferenced": true
        }
      ],
      "response":{
        "_all_": "",
        "refdLineupList":{
          "typeId": "mcdLineupArtist",
          "query": [
            {
              "field": "lineupInfoSeq",
              "method": "referenceJoin",
              "value": "lineupInfoSeq_matching={{:lineupInfoSeq}}"
            },
            {
              "field": "langCd",
              "method": "matching",
              "value": "{{:langCd}}"
            },
            {
              "includeReferenced": true,
              "sorting": "sortOdrg"
            }
          ]
        },
        "lineupNum":{
          "resultType": "size",
          "field": "refdLineupList"
        }
      }
    }
  }
]
