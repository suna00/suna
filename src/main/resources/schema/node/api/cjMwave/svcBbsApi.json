  {
    "typeId": "apiConfig",
    "category": "svcBbs",
    "apiId": "IfNtb002",
    "apiName": "고객 공지사항 상세 조회",
    "method": "GET",
    "parameters": [
      {
        "paramName": "searchKeyword",
        "required": false
      },
      {
        "paramName": "ntfBbsSeq",
        "required": true
      },
      {
        "paramName": "svcTermlDivCd",
        "required": false
      },
      {
        "paramName": "ntfLargeCtgCd",
        "required": false
      }
    ],
    "config": {
      "typeId": "ntfBbs",
      "id": "{{:ntfBbsSeq}}",
      "query":[
        {
          "referenceView": true
        }
      ],
      "response": {
        "ntfBbsSeq": "",
        "ntfBbsNm": "",
        "ntfBbsTgtCd": "",
        "ntfBbsTypeCd": "",
        "ntfLargeCtgCd": "",
        "ntfMidCtgCd": "",
