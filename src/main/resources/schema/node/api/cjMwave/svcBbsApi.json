        "pstngStDt": "",
        "pstngFnsDt": "",
        "ntfSbst": "",
        "hitNum": "retvNum",
        "owner":"",
        "created":"",
        "nextItem": {
          "typeId": "ntfBbs",
          "query": [
            {
              "field": "ntfBbsSeq",
              "method": "under",
              "value": "{{:ntfBbsSeq}}"
            },
            {
              "sorting": "ntfBbsSeq desc",
              "limit": "1"
            }
          ],
          "response": {
            "ntfBbsSeq": "",
            "ntfLargeCtgCd": "",
            "ntfMidCtgCd": "",
            "ntfBbsNm": "",
            "created": ""
          }
        },
        "preItem": {
          "typeId": "ntfBbs",
          "query": [
            {
              "field": "ntfBbsSeq",
              "method": "excess",
              "value": "{{:ntfBbsSeq}}"
            },
            {
              "sorting": "ntfBbsSeq asc",
              "limit": "1"
            }
          ],
          "response": {
            "ntfBbsSeq": "",
            "ntfLargeCtgCd": "",
            "ntfMidCtgCd": "",
            "ntfBbsNm": "",
            "created": ""
          }
        }
      }
    }
