        "qnaFild": "{{:qnaFildCd}}",
        "qstnrEmail": "{{:qstnrEmail}}",
        "qstnSbst": "{{:qstnSbst}}",
        "qstnAtcFilePath": "{{:qstnAtcFile}}",
        "trtSttusCd": "1"
      }
    }
  },
