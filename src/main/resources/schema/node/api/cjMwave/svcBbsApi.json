[
  {
    "typeId": "apiConfig",
    "category": "svcBbs",
    "apiId": "IfNtb001",
    "apiName": "고객 공지사항 목록 조회",
    "method": "GET",
    "parameters": [
      {
        "paramName": "page",
        "required": false
      },
      {
        "paramName": "pageSize",
        "required": false
      },
      {
        "paramName": "sorting",
        "required": false
      },
      {
        "paramName": "langCd",
        "required": false
      },
      {
        "paramName": "svcTermlDivCd",
        "required": true
      },
      {
        "paramName": "ntfLargeCtgCd",
        "required": false
      },
      {
        "paramName": "searchKeyword",
        "required": false
      }
    ],
    "config": {
      "typeId": "ntfBbs",
      "query": [
        {
          "field": "showDvcCd",
          "method": "matching",
          "value": "{{:svcTermlDivCd}}"
        },
        {
          "field": "ctgCd1",
          "method": "matching",
          "value": "{{:ntfLargeCtgCd}}"
        },
        {
          "field": "showYn",
          "method": "matching",
          "value": "true"
        },
        {
          "sorting": "{{:sorting}}"
        },
        {
          "page": "{{:page}}",
          "pageSize": "{{:pageSize}}"
        }
      ]
    }
  },
  {
    "typeId": "apiConfig",
    "category": "svcBbs",
    "apiId": "IfNtb002",
    "apiName": "고객 공지사항 상세 조회",
    "method": "GET",
    "parameters": [
      {
        "paramName": "langCd",
        "required": false
      },
      {
        "paramName": "ntfBbsSeq",
        "required": true
      }
    ],
    "config": {
      "typeId": "ntfBbs",
      "id": "{{:ntfBbsSeq}}",
      "response": {
        "ntfBbsSeq": "",
        "ntfBbsNm": "",
        "ntfBbsTgtCd": "",
        "ntfBbsTypeCd": "",
        "ctgCd1": "",
        "ctgCd2": "",
        "showDvcCd": "",
        "pstngStDt": "",
        "pstngFnsDt": "",
        "ntfSbst": "",
        "retvNum": "",
        "preItem": {
          "typeId": "ntfBbs",
          "query": [
            {
              "field": "ntfBbsSeq",
              "method": "under",
              "value": "{{:ntfBbsSeq}}"
            },
            {
              "sorting": "ntfBbsSeq desc",
              "limit": "1"
            }
          ],
          "response": {
            "ntfBbsSeq": "",
            "ctgCd1": "",
            "ctgCd2": "",
            "ntfBbsNm": "",
            "created": ""
          }
        },
        "nextItem": {
          "typeId": "ntfBbs",
          "query": [
            {
              "field": "ntfBbsSeq",
              "method": "excess",
              "value": "{{:ntfBbsSeq}}"
            },
            {
              "sorting": "ntfBbsSeq asc",
              "limit": "1"
            }
          ],
          "response": {
            "ntfBbsSeq": "",
            "ctgCd1": "",
            "ctgCd2": "",
            "ntfBbsNm": "",
            "created": ""
          }
        }
      }
    }
  },
  {
    "typeId": "apiConfig",
    "category": "svcBbs",
